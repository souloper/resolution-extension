{  
  "manifest_version": 3,
  "name": "Interactive Info Dropdown",
  "version": "1.0",
  "description": "An innovative extension that displays dropdown options with interactive information.",
  "permissions": ["clipboardWrite", "activeTab", "scripting"],
  "host_permissions": ["https://walgreens.onbmc.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/images/icon128.png",
    "icons": {
      "16":  "/images/icon16.png",
      "32":  "/images/icon32.png",
      "48":  "/images/icon48.png",
      "128": "/images/icon128.png"
  }
  }
}